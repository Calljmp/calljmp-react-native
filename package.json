{
  "name": "@calljmp/react-native",
  "version": "0.0.52",
  "description": "Calljmp is a secure backend-as-a-service (BaaS) for React Native apps, with auth, storage, SQL access, and attestation support.",
  "main": "./dist/index.cjs.js",
  "keywords": [
    "calljmp",
    "react-native",
    "expo",
    "ai",
    "agents",
    "workflows"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Calljmp/calljmp-react-native.git"
  },
  "author": "Calljmp <info@calljmp.com> (https://calljmp.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Calljmp/calljmp-react-native/issues"
  },
  "homepage": "https://calljmp.com",
  "codegenConfig": {
    "name": "RNCalljmpSpec",
    "type": "modules",
    "jsSrcsDir": "src/specs",
    "android": {
      "javaPackageName": "com.calljmp"
    }
  },
  "dependencies": {
    "@calljmp/agent": "^0.0.2"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-native": ">=0.72.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": false
    }
  },
  "module": "./dist/index.esm.js",
  "types": "./dist/index.esm.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.esm.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.cjs.js"
    },
    "./package.json": "./package.json",
    "./app.plugin.js": "./dist/app.plugin.js",
    "./babel-plugin": {
      "types": "./dist/lib/babel-plugin.d.ts",
      "import": "./dist/lib/babel-plugin.js",
      "require": "./dist/lib/babel-plugin.js",
      "default": "./dist/lib/babel-plugin.js"
    }
  },
  "devDependencies": {
    "@babel/types": "^7.28.4",
    "@nx/rollup": "20.4.0",
    "@react-native/babel-preset": "^0.81.0",
    "@tsconfig/node-lts": "^22.0.2",
    "rollup": "^4.14.0",
    "zod": "^3.22.3"
  },
  "nx": {
    "name": "sdk-react-native",
    "sourceRoot": "libs/sdk-react-native/src",
    "projectType": "library",
    "tags": [],
    "targets": {
      "common:sync": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": ".",
          "commands": [
            "rm -rf ./libs/sdk-react-native/src/common && mkdir -p ./libs/sdk-react-native/src/common && cp -r ./libs/common/src/* ./libs/sdk-react-native/src/common/"
          ]
        }
      },
      "publish:git": {
        "executor": "nx:run-commands",
        "dependsOn": [
          "common:sync",
          "typecheck"
        ],
        "options": {
          "cwd": "libs/sdk-react-native",
          "commands": [
            "./tools/git-publish.sh"
          ]
        }
      },
      "publish:npm": {
        "executor": "nx:run-commands",
        "dependsOn": [
          "build"
        ],
        "options": {
          "cwd": "libs/sdk-react-native/dist",
          "commands": [
            "npm publish --access public"
          ]
        }
      },
      "build:babel:plugin": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "libs/sdk-react-native",
          "commands": [
            "tsc --build babel-plugin"
          ]
        }
      },
      "build:plugin": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "libs/sdk-react-native",
          "commands": [
            "tsc --build expo-plugin"
          ]
        }
      },
      "build": {
        "executor": "nx:run-commands",
        "dependsOn": [
          "bundle"
        ],
        "options": {
          "cwd": "libs/sdk-react-native/dist",
          "commands": [
            "../tools/package.sh"
          ]
        }
      },
      "bundle": {
        "executor": "@nx/rollup:rollup",
        "outputs": [
          "{options.outputPath}"
        ],
        "dependsOn": [
          "common:sync",
          "build:plugin",
          "build:babel:plugin"
        ],
        "options": {
          "outputPath": "libs/sdk-react-native/dist",
          "tsConfig": "libs/sdk-react-native/tsconfig.lib.json",
          "project": "libs/sdk-react-native/package.json",
          "entryFile": "libs/sdk-react-native/src/index.ts",
          "format": [
            "esm",
            "cjs"
          ],
          "external": [
            "react/jsx-runtime",
            "react-native",
            "react",
            "react-dom"
          ],
          "rollupConfig": "@nx/react/plugins/bundle-rollup",
          "assets": [
            {
              "glob": "libs/sdk-react-native/package.json",
              "input": ".",
              "output": "."
            },
            {
              "glob": "libs/sdk-react-native/README.md",
              "input": ".",
              "output": "."
            },
            {
              "glob": "libs/sdk-react-native/LICENSE",
              "input": ".",
              "output": "."
            },
            {
              "glob": "libs/sdk-react-native/android",
              "input": ".",
              "output": "."
            },
            {
              "glob": "libs/sdk-react-native/ios",
              "input": ".",
              "output": "."
            },
            {
              "glob": "libs/sdk-react-native/Calljmp.podspec",
              "input": ".",
              "output": "."
            },
            {
              "glob": "libs/sdk-react-native/src/specs/**/*.ts",
              "input": ".",
              "output": "./src/specs/"
            },
            {
              "glob": "libs/sdk-react-native/expo-plugin/build/**",
              "input": ".",
              "output": "./lib/"
            },
            {
              "glob": "libs/sdk-react-native/babel-plugin/build/**",
              "input": ".",
              "output": "./lib/"
            }
          ]
        }
      },
      "typecheck": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "libs/sdk-react-native",
          "commands": [
            "tsc -p tsconfig.lib.json --noEmit"
          ]
        }
      }
    }
  }
}